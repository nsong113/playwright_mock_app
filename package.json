{
  "name": "qa-mock-portfolio",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend",
    "mock-server"
  ],
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:frontend\"",
    "build": "yarn workspaces run build",
    "dev:frontend": "yarn workspace qa-mock-portfolio-frontend dev",
    "dev:server": "yarn workspace qa-mock-portfolio-server dev",
    "build:frontend": "yarn workspace qa-mock-portfolio-frontend build",
    "build:server": "yarn workspace qa-mock-portfolio-server build"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
